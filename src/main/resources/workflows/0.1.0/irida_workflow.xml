<?xml version="1.0" encoding="UTF-8"?>
<iridaWorkflow>
  <id>152c9062-d39b-4fce-8234-2353223afc90</id>
  <name>SpeciesAbundance</name>
  <version>0.1.0</version>
  <analysisType>SPECIES_ABUNDANCE</analysisType>
  <inputs>
    <sequenceReadsPaired/>
    <requiresSingleSample>true</requiresSingleSample>
  </inputs>
  <parameters>
    <parameter name="kraken2-1-confidence" defaultValue="0.0">
      <toolParameter toolId="toolshed.g2.bx.psu.edu/repos/iuc/kraken2/kraken2/2.0.8_beta+galaxy0" parameterName="confidence" label="Confidence" type="float"/>
    </parameter>
    <parameter name="kraken2-1-kraken2_database" required="true">
      <dynamicSource>
        <galaxyToolDataTable name="kraken2_databases" displayColumn="name" parameterColumn="value" />
      </dynamicSource>
      <toolParameter toolId="toolshed.g2.bx.psu.edu/repos/iuc/kraken2/kraken2/2.0.8_beta+galaxy0" parameterName="kraken2_database"/>
    </parameter>
    <parameter name="kraken2-1-use_names" defaultValue="false">
      <toolParameter toolId="toolshed.g2.bx.psu.edu/repos/iuc/kraken2/kraken2/2.0.8_beta+galaxy0" parameterName="use_names" label="Print scientific names instead of just taxids" type="boolean"/>
    </parameter>
    <parameter name="kraken2-1-split_reads" defaultValue="false">
      <toolParameter toolId="toolshed.g2.bx.psu.edu/repos/iuc/kraken2/kraken2/2.0.8_beta+galaxy0" parameterName="split_reads" label="Split classified and unclassified outputs?" type="boolean"/>
    </parameter>
    <parameter name="kraken2-1-single_paired.single_paired_selector" defaultValue="collection">
      <toolParameter toolId="toolshed.g2.bx.psu.edu/repos/iuc/kraken2/kraken2/2.0.8_beta+galaxy0" parameterName="single_paired.single_paired_selector" label="Single or paired reads" type="select"/>
    </parameter>
    <parameter name="kraken2-1-min_base_quality" defaultValue="0">
      <toolParameter toolId="toolshed.g2.bx.psu.edu/repos/iuc/kraken2/kraken2/2.0.8_beta+galaxy0" parameterName="min_base_quality" label="Minimum Base Quality" type="integer"/>
    </parameter>
    <parameter name="kraken2-1-report.report_zero_counts" defaultValue="false">
      <toolParameter toolId="toolshed.g2.bx.psu.edu/repos/iuc/kraken2/kraken2/2.0.8_beta+galaxy0" parameterName="report.report_zero_counts" label="Report counts for ALL taxa, even if counts are zero" type="boolean"/>
    </parameter>
    <parameter name="kraken2-1-report.create_report" defaultValue="false">
      <toolParameter toolId="toolshed.g2.bx.psu.edu/repos/iuc/kraken2/kraken2/2.0.8_beta+galaxy0" parameterName="report.create_report" label="Print a report with aggregrate counts/clade to file" type="boolean"/>
    </parameter>
    <parameter name="kraken2-1-quick" defaultValue="false">
      <toolParameter toolId="toolshed.g2.bx.psu.edu/repos/iuc/kraken2/kraken2/2.0.8_beta+galaxy0" parameterName="quick" label="Enable quick operation" type="boolean"/>
    </parameter>
    <parameter name="kraken2-1-report.use_mpa_style" defaultValue="false">
      <toolParameter toolId="toolshed.g2.bx.psu.edu/repos/iuc/kraken2/kraken2/2.0.8_beta+galaxy0" parameterName="report.use_mpa_style" label="Format report output like Kraken 1's kraken-mpa-report" type="boolean"/>
    </parameter>
    <parameter name="bracken-2-threshold" defaultValue="10">
      <toolParameter toolId="testtoolshed.g2.bx.psu.edu/repos/dfornika/bracken/est_abundance/2.5+galaxy0" parameterName="threshold"/>
    </parameter>
    <parameter name="bracken-2-kmer_distr" required="true">
      <dynamicSource>
        <galaxyToolDataTable name="bracken_databases" displayColumn="name" parameterColumn="value" />
      </dynamicSource>
      <toolParameter toolId="testtoolshed.g2.bx.psu.edu/repos/dfornika/bracken/est_abundance/2.5+galaxy0" parameterName="kmer_distr"/>
    </parameter>
  </parameters>
  <outputs>
    <output name="species_abundance" fileName="species_abundance.tsv" />
  </outputs>
  <toolRepositories>
    <repository>
      <name>kraken2</name>
      <owner>iuc</owner>
      <url>https://toolshed.g2.bx.psu.edu</url>
      <revision>328c607150ff</revision>
    </repository>
    <repository>
      <name>bracken</name>
      <owner>dfornika</owner>
      <url>https://testtoolshed.g2.bx.psu.edu</url>
      <revision>6e90882d79fd</revision>
    </repository>
  </toolRepositories>
</iridaWorkflow>
